<h1><%= @image.user.name %>'s image</h1>
<br>
<%= image_tag @image.img_file.url, class:"show-image" %>

<p><%= link_to "back", user_path(@image.user) %></p>
<% if session[:user_id] == @image.user_id %>
<p><%= link_to "delete image", user_image_path(@image.user, @image),
                            :data => {:method => "delete"}, class:'btn btn-default btn-sm' %></p>
<% end %>
